#Code CracklePop
#Write a program that prints out the numbers 1 to 100 (inclusive). 
#If the number is divisible by 3, print Crackle instead of the number. 
#If it's divisible by 5, print Pop. If it's divisible by both 3 and 5, print CracklePop. You can use any language.

#1、C language
#include <stdio.h>

int main()
{
        {        
        int m = 100 ;

        for(int  i = 0; i <= m; i++)

                if (i%(3*5) == 0)
                {
                        printf("CracklePop") ;
                }else if(i%3 == 0)
                {
                        printf("Crackle") ;
                }else if (i%5 == 0)
                {
                        printf("Pop") ;
                }else
                {
                        printf("%d", i) ;
                }
        }
        return 0;
}



#2、C++ language

#include <iostream>
using namespace std;

int main() {
    
    
        int m = 100 ;

        for(int i = 0; i <= m; i++)
        {
                if (i%(3*5) == 0)
                {
                        cout <<"CracklePop" ;
                }else if(i%3 == 0)
                {
                        cout <<"Crackle";
                }else if (i%5 == 0)
                {
                        cout<<"Pop" ;
                }else
                {
                        cout <<i ;
                }
        }
        return 0;
	
}


#3、python

#!/usr/bin/python
# -*- coding: utf-8 -*-

for i in range (101):
  if i%(3*5)==0:
    print("CracklePop") 
  elif(i%3==0):
    print("Crackle") 
  elif (i%5==0):
    print("Pop") 
  else:
    print (i) 
                
        


